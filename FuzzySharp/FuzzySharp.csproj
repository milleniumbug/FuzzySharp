<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.0;netstandard1.6;netstandard2.0;net45</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Jacob Bayer</Authors>
    <Description>Fuzzy matcher based on FuzzyWuzzy algorithm from SeatGeek</Description>
    <PackageTags>Fuzzy String Matching FuzzyWuzzy FuzzySharp</PackageTags>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
	<RepositoryUrl>https://github.com/JakeBayer/FuzzySharp</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
    <Company />
    <Version>1.0.2</Version>
    <PackageReleaseNotes>Compatibility with .netstandard and more versions of core and .net</PackageReleaseNotes>
	<PublishRepositoryUrl>true</PublishRepositoryUrl>
  </PropertyGroup>

</Project>
